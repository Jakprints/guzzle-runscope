{
    "name": "mike27cubes/runscope-guzzle-plugin",
    "description": "This package adds Runscope support to the Guzzle 5.x PHP library.",
    "homepage": "https://github.com/mike27cubes/guzzle-runscope",
    "license": "MIT",
    "authors": [
        {
            "name": "Runscope",
            "homepage": "https://www.runscope.com",
            "email": "help@runscope.com"
        },
        {
            "name": "Luis Cordova",
            "homepage": "http://craftitonline.com"
        },
        {
            "name": "Mike Hickey",
            "homepage": "http://www.27cubes.com"
        }
    ],
    "keywords": ["runscope", "guzzle"],
    "require": {
        "php": ">=5.4.0",
        "psr/log": "1.0.0",
        "guzzlehttp/guzzle": "~5.0"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev",
        "monolog/monolog": "1.6.*",
        "raulfraile/ladybug": "~1.0",
        "phpunit/phpunit": "4.4.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {"psr-0": {"Runscope": "src/"}}
}
