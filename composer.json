{
    "name": "runscope/guzzle-plugin",
    "description": "This package adds Runscope support to the Guzzle PHP library.",
    "homepage": "https://github.com/runscope/guzzle-runscope",
    "license": "MIT",
    "authors": [
        {
            "name": "Runscope",
            "homepage": "https://www.runscope.com",
            "email": "help@runscope.com"
        },
        {
            "name": "Luis Cordova",
            "homepage": "http://craftitonline.com"
        }
    ],
    "keywords": ["runscope", "guzzle"],
    "require": {
        "php": ">=5.3.0",
        "psr/log": "1.0.0",
        "guzzle/guzzle": ">=v3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev",
        "monolog/monolog": "1.6.*",
        "raulfraile/ladybug": "~1.0"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {"psr-0": {"Runscope": "src/"}}
}